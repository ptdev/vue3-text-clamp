    "@vue/compiler-sfc": "^3.2.37",
    "highlight.js": "^11.6.0",
    "less": "^4.1.3",
    "rollup": "^2.77.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "rollup-plugin-vue": "^6.0.0",
    "spectre.css": "^0.5.9",
